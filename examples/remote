#!/usr/bin/env python
# lunch example config file.
# Copy this to ~/.lunchrc
#
# In this example, we run commands on a remote host, via SSH.

HOST = "brrr"
USER = "miville"

add_command("xeyes", env={"DISPLAY":":0.0"}, user=USER, host=HOST, title="remote_xeyes")
add_command("xlogo", env={"DISPLAY":":0.0"}, user=USER, host=HOST, title="remote_xeyes")
